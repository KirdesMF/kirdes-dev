<svg aria-hidden="true" focusable="false" class="pointer-events-none absolute w-0 h-0">
  <defs>
    <pattern
      id="pattern-frame-stripes"
      patternUnits="userSpaceOnUse"
      width="8"
      height="8"
      patternTransform="rotate(45)"
    >
      <rect width="8" height="8" fill="transparent"></rect>
      <line x1="0" y1="0" x2="0" y2="8" stroke="currentColor" stroke-width="1"></line>
    </pattern>

    <pattern
      id="pattern-frame-stripes-wireframe"
      patternUnits="userSpaceOnUse"
      width="8"
      height="8"
      patternTransform="rotate(45)"
    >
      <rect width="8" height="8" fill="transparent"></rect>
      <line
        x1="0"
        y1="0"
        x2="0"
        y2="8"
        stroke="currentColor"
        stroke-width="0.75"
        stroke-dasharray="5 2"
        stroke-opacity="1"></line>
    </pattern>
  </defs>
</svg>
